{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@components": ["components"],
      "@utils/*": ["utils/*"],
      "@utils": ["utils"],
      "@styles/*": ["styles/*"],
      "@styles": ["styles"],
      "@routes/*": ["routes/*"],
      "@routes": ["routes"],
      "@router/*": ["router/*"],
      "@router": ["router"]
    }
  },
  "include": ["src"]
}
