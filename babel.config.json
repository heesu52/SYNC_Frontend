{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-typescript"
    ],
    [
      "@svgr/babel-preset",
      {
        "svgProps": {
          "fill": "currentColor",
          "color": "currentColor",
          "size": "{size}",
          "className": "{className}"
        }
      }
    ]
  ]
}